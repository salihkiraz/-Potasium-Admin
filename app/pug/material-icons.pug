extends layouts/base
block styles
	link(rel='stylesheet', href='https://fonts.googleapis.com/icon?family=Material+Icons')
	link(rel='stylesheet', href='libs/lobibox/dist/css/lobibox.min.css')
block page-content
	.row
		.col-lg-12.col-md-12
			.card
				.card-body
					.ui.fluid.big.icon.input
						input#search.prompt(type='text', placeholder='Search ')
						i.search.icon
					.ui.hidden.divider
					.change-icon-color
						ul.color-list
							li.color-one
							li.color-two
							li.color-three
							li.color-four
							li.color-five
							li.color-six
							li.color-seven
							li.color-eight
							li.color-nine
							li.color-ten
	div#container_sui_icon.row
block scripts
	script(src="./libs/clipboard/dist/clipboard.min.js")
	script(src="./libs/lobibox/dist/js/lobibox.min.js")
	script(src="./js/pages/material-icons.min.js")