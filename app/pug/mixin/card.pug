mixin home-dashboard-control-card(title,icon,checked)
    .card.home-dashboard-control-card
        h6.card-subtitle.mb-2.text-muted
            | #{title}
        .card-body
            if(checked=="true")
                i.icon.iconfont(class="icon-"+icon+"",style="color:#266DD3")
                label.switch.home-control-switch
                    input(type='checkbox',checked="true")
                    span
            else
                i.icon.iconfont(class="icon-"+icon+"",style="color:#6c757d")
                label.switch.home-control-switch
                    input(type='checkbox')
                    span
