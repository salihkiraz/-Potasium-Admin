include grid.pug
mixin basiccard(title,hidden="",textPosition="")
  .card(class=hidden)
    .card-header
      h4.card-title=title
      a.heading-elements-toggle
        i.fa.fa-ellipsis.font-medium-3
      .heading-elements
        ul.list-inline.mb-0
          li
            a(data-action='copy')
              i.icon.iconfont.icon-xitongcaidan
          li
            a(data-action='collapse')
              i.icon.iconfont.icon-BAI-qiehuan
          li
            a(data-action='reload')
              i.icon.iconfont.icon-shuaxin
          li
            a(data-action='expand')
              i.icon.iconfont.icon-saoyisao
          li
            a(data-action='close')
              i.icon.iconfont.icon-guanbijiantou
    .collapse.show
      .card-body
        .ui.inverted.dimmer
          .ui.text.loader Loading
        .card-block(class=textPosition)
          block

mixin basiccardwithactions(title)
    .card.home-dashboard-card
        .card-header
            h4.card-title=title
        .card-body
            block

mixin colorcards(color,colorname,colorcode)
