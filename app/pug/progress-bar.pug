			+basiccardwithactions("Semantic-Ui Progres Bar")
					.ui.green.progress(data-percent='40')
						.bar
		+col(12,12,12,12,4)     
			+basiccardwithactions("Semantic-Ui Progres Bar")
					.ui.blue.progress(data-percent='99')
						.bar
		+col(12,12,12,12,4)     
			+basiccardwithactions("Semantic-Ui Progres Bar")
					.ui.violet.progress(data-percent='33')
						.bar
		+col(12,12,12,12,4)     
			+basiccardwithactions("Semantic-Ui Progres Bar")
					.ui.purple.progress(data-percent='50')
						.bar
block scripts
	script.
		$('#example1').progress();
		$('#example4').progress('increment');
		$('#example5').progress('increment');
		$('#example6').progress({
			label: 'ratio',
			text: {
				ratio: '{value} de {total}'
			}
		});
		$('.semantic_ui_progress >.ui.progress').progress();
