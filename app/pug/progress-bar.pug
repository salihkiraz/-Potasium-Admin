			+basiccardwithactions("Semantic-Ui Progres Bar")
				.ui.progress
					.bar
		+col(12,12,12,12,4)     
			+basiccardwithactions("Semantic-Ui Progres Bar")
				#example4.ui.indicating.progress(data-value='1', data-total='20')
					.bar
						.progress
					.label Adding Photos
		+col(12,12,12,12,4)     
			+basiccardwithactions("Semantic-Ui Progres Bar")
				#example6.ui.progress(data-value='15', data-total='20')
					.bar
						.progress
					.label Carga de archivos
